############# Triplet Loss ###############
layer {
  name: "tripletloss"
  type: "Python"
  bottom: "feat"
  bottom: "feat_p"
  bottom: "feat_n"
  top: "loss"
  python_param {
    module: "tripletloss_layer"
    layer: "TripletLossLayer"
    param_str: '{\"margin\": 1.0}'
  }
  include{
        phase: TRAIN
  }
}